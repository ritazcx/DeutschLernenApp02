{
  "name": "deutschflow-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "test": "jest --config jest.config.integration.js",
    "test:unit": "jest --config jest.config.unit.js",
    "test:integration": "jest --config jest.config.integration.js",
    "test:watch": "jest --config jest.config.integration.js --watch",
    "test:watch:unit": "jest --config jest.config.unit.js --watch",
    "test:coverage": "jest --config jest.config.integration.js --coverage",
    "test:local": "ts-node test-local.ts"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "better-sqlite3": "^8.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pdf-parse": "^2.4.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.5.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.6"
  }
}
